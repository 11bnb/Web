{
  "name": "11bnb_web",
  "version": "0.9.0",
  "description": "11bnb_web",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf *.json.gzip",
    "start": "dora --port 61101 --plugins \"webpack?config=scripts/webpack.config.development.js,webpack-hmr,livereload?enableJs=true&injectHost=127.0.0.1\"",
    "build": "npm run build:desktop && npm run build:mobile && npm run clean",
    "build:desktop": "atool-build --config scripts/webpack.config.production.desktop.js -o ./dist",
    "build:mobile": "atool-build --config scripts/webpack.config.production.mobile.js -o ./dist",
    "build:analyze": "atool-build --config scripts/webpack.config.analyze.js -o ./dist --verbose && npm run clean",
    "post": "echo no maven command",
    "pages": "npm run pages:checkout && npm run pages:build && npm run pages:cleanall && npm run pages:publish",
    "pages:checkout": "git add . && git stash && git checkout gh-pages && git add . && git checkout master ./",
    "pages:build": "atool-build --config scripts/webpack.config.production.js -o ./",
    "pages:cleanall": "npm run clean && rm -rf config/ libs/ scripts/ server/ src/ views/",
    "pages:publish": "git add . && git commit -m 'update pages' --amend -n && git push -f",
    "postpages:publish": "git checkout master && git stash pop",
    "lint": "eslint --ext .js,.jsx libs/ server/ src/",
    "proginn": "node scripts/proginn.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/11bnb/Web.git"
  },
  "author": "yeliex",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/11bnb/Web/issues"
  },
  "homepage": "https://github.com/11bnb/Web#readme",
  "dependencies": {
    "async-validator": "^1.6.6",
    "autofetch": "^1.1.5",
    "classnames": "^2.2.5",
    "glob": "^7.1.1",
    "handlebars": "^4.0.5",
    "koa": "^1.2.4",
    "koa-body": "^1.6.0",
    "koa-cookie": "^1.0.0",
    "koa-response": "^1.0.3",
    "koa-router": "^5.4.0",
    "koa-session": "^3.4.0",
    "lodash": "^4.16.6",
    "moment": "^2.17.1",
    "node.date-time": "^1.1.1",
    "node.os": "^1.2.4",
    "rc-banner-anim": "^0.5.3",
    "rc-queue-anim": "^0.12.5",
    "rc-tween-one": "^0.11.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-visibility-sensor": "^3.4.0",
    "redux": "^3.6.0",
