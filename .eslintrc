{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env":{
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": [0],
    "arrow-parens": [0, "always"],
    "comma-dangle":[2, "never"],
    "spaced-comment": [0],
    "global-require": [0],
    "max-len":[0],
    "no-console": ["error", { "allow": ["log", "warn", "error"] }],
    "no-constant-condition": [0],
    "no-underscore-dangle": ["error", { "allow": ["_GET", "_POST", "_id"] }],
    "no-unused-expressions": [0, { "allowTernary": true }],
    "no-unused-vars": [0],
    "no-use-before-define": ["error", { "functions": true }],
    "no-nested-ternary": [0],
    "no-empty": [0],
    "no-fallthrough": [0],
    "no-param-reassign": [0],
    "no-plusplus": [0],
    "semi": [0],
    "react/wrap-multilines": [0],
    "react/no-multi-comp": [0],
    "react/jsx-no-bind": [0],
    "react/jsx-closing-bracket-location": [0, { "nonEmpty": "after-props", "selfClosing": "line-aligned" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "react/forbid-prop-types": [0],
    "react/prefer-stateless-function": [0],
    "jsx-a11y/anchor-has-content": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "jsx-a11y/label-has-for": [0]

  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./scripts/webpack.config.eslint.js"
      }
    }
  }
}
